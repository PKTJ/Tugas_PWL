{
    "name": "arbinandri/tugas-ajax-07051",
    "description": "Live Search Mahasiswa dengan AJAX dan Export Excel/PDF",
    "type": "project",
    "keywords": ["php", "ajax", "live-search", "excel-export", "pdf-export", "bootstrap"],
    "license": "MIT",
    "authors": [
        {
            "name": "ARBINANDRI",
            "email": "arbinandri@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phpoffice/phpspreadsheet": "^1.29",
        "dompdf/dompdf": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "install-deps": "composer install --no-dev --optimize-autoloader",
        "update-deps": "composer update --no-dev --optimize-autoloader"
    }
}
